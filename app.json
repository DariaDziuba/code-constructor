{
  "expo": {
    "name": "code-configurator",
    "slug": "code-configurator",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/code-configurator.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/configurator-loader.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expocode.configurator.com"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/code-configurator.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "expocode.configurator.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
